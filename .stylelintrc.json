{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "tailwind",
        "responsive",
        "screen"
      ]
    }],
    "block-no-empty": null,
    "unit-allowed-list": ["em", "rem", "s","px","%","deg","ms"]
  }
}